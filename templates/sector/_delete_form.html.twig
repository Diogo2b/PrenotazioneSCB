<form method="post" action="{{ path('app_sector_delete', {'id': sector.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ sector.id) }}">
	<button class="btn btn-danger">Supprimer</button>
</form>
