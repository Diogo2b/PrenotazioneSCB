{% extends 'base.html.twig' %}

{% block title %}Sector
{% endblock %}

{% block body %}
	<h1>Sector</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ sector.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ sector.name }}</td>
			</tr>
			<tr>
				<th>Sigle</th>
				<td>{{ sector.sigle }}</td>
			</tr>
			<tr>
				<th>NumberedSeats</th>
				<td>{{ sector.numberedSeats ? 'Yes' : 'No' }}</td>
			</tr>
			<tr>
				<th>Capacity</th>
				<td>{{ sector.capacity }}</td>
			</tr>
			<tr>
				<th>AvailableForSale</th>
				<td>{{ sector.availableForSale ? 'Yes' : 'No' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_sector_index') }}" class="btn btn-primary">back to list</a>

	<a href="{{ path('app_sector_edit', {'id': sector.id}) }}" class="btn btn-warning">edit</a>

	{{ include('sector/_delete_form.html.twig') }}
{% endblock %}
